<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="module_category_openeducat_fees" model="ir.module.category">
            <field name="name">OpenEduCat Fees</field>
            <field name="description">Helps you manage your institutes different-different users.</field>
            <field name="sequence">18</field>
        </record>

        <record id="group_openeducat_fees_user" model="res.groups">
            <field name="name">Fees User</field>
            <field name="category_id" ref="module_category_openeducat_fees"/>
            <field name="users" eval="[(4, ref('base.user_admin'))]"/>
            <field name="implied_ids"
                   eval="[(4, ref('base.group_user')),(4, ref('openeducat_core.group_op_faculty'))]"/>
        </record>

        <record id="group_op_fees_admin" model="res.groups">
            <field name="name">Fees Manager</field>
            <field name="category_id" ref="module_category_openeducat_fees"/>
            <field name="users" eval="[(4, ref('base.user_admin'))]"/>
            <field name="implied_ids"
                   eval="[(4, ref('group_openeducat_fees_user')),(4, ref('openeducat_core.group_op_back_office_admin')),(4, ref('base.group_partner_manager')),(4, ref('account.group_account_invoice'))]"/>
        </record>

        <record id="openeducat_core.group_op_back_office_admin" model="res.groups">
            <field name="name">Back Office Admin</field>
            <field name="category_id" ref="openeducat_core.module_category_openeducat"/>
            <field name="users" eval="[(4, ref('base.user_admin'))]"/>
            <field name="implied_ids"
                   eval="[(4, ref('openeducat_fees.group_op_fees_admin')),(4, ref('base.group_system'))]"/>
        </record>
<!--        <record id="openeducat_core.group_op_back_office" model="res.groups">-->
<!--            <field name="implied_ids"-->
<!--                   eval="[(4, ref('openeducat_core.group_op_faculty')),(4, ref('base.group_partner_manager')),(4, ref('account.group_account_invoice'))]"/>-->
<!--        </record>-->
    </data>
</odoo>
